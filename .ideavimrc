source ~/.config/nvim/init.vim
set surround
set multiple-cursors
set commentary
let mapleader = " "
nmap <leader>p :action ParameterInfo<CR>
nmap <leader>b :action GotoDeclaration<CR>
nmap <leader>f :action Find<CR>
nmap g] :action GotoImplementation<CR>
nmap <leader>i :action GotoImplementation<CR>
nnoremap <C-,> :action CollapseRegionRecursively<CR>
nnoremap , :action ExpandRegionRecursively<CR>
